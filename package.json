{
  "name": "angular-electron-starter",
  "version": "0.9.0",
  "description": "Angular Electron Starter",
  "main": "electron.js",
  "scripts": {
    "prestart": "npm run clean",
    "start": "webpack --display-error-details --progress --watch",
    "prestart:aot": "npm run clean",
    "start:aot": "webpack --display-error-details --progress --env.aot --watch",
    "electron": "npm run build:electron:main && electron ./build --serve",
    "build:electron:main": "tsc ./src/electron.ts --outDir ./build && copyfiles package.json ./build && cd ./build && npm install --prod && cd ..",
    "prebuild": "npm run clean",
    "build": "webpack --display-error-details --progress && npm run build:electron:main",
    "prebuild:prod": "npm run clean",
    "build:prod": "webpack --display-error-details --env.aot -p --progress && npm run build:electron:main",
    "clean": "rimraf build",
    "app:linux": "npm run build:prod && node package.js --asar --platform=linux --arch=x64",
    "app:windows": "npm run build:prod && node package.js --asar --platform=win32 --arch=ia32",
    "app:mac": "npm run build:prod && node package.js --asar --platform=darwin --arch=x64",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet && npm run build",
    "e2e": "protractor ./protractor.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkuri/angular-electron-starter.git"
  },
  "keywords": [
    "Angular",
    "Angular2+",
    "Angular4",
    "Electron",
    "Starter",
    "Seed"
  ],
  "author": "Jan Kuri <jkuri88@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkuri/angular-electron-starter/issues"
  },
  "homepage": "https://github.com/jkuri/angular-electron-starter#readme",
  "devDependencies": {
    "@angular/animations": "^4.3.1",
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@ngtools/webpack": "^1.5.2",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.15",
    "circular-dependency-plugin": "^3.0.0",
    "codelyzer": "^3.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "electron": "^1.6.11",
    "electron-installer-debian": "^0.5.2",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^8.7.2",
    "electron-reload": "^1.2.1",
    "electron-winstaller": "^2.6.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "^4.1.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "minimist": "^1.2.0",
    "node-sass": "^4.5.3",
    "protractor": "^5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.2",
    "sass-loader": "^6.0.6",
    "ts-node": "^3.2.1",
    "tslint": "^5.5.0",
    "typescript": "2.4.2",
    "uglify-js": "^3.0.26",
    "webpack": "^3.3.0",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.14"
  }
}
